{
  "name": "transform",
  "publisher": "yexi",
  "displayName": "transform",
  "description": "transform code when change code",
  "version": "0.1.4",
  "engines": {
    "vscode": "^1.44.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:shuerguo999/vscode-transform.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/.transform.json"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "transform",
      "properties": {
        "fileType": {
          "type": "string",
          "default": ".{ts,js}",
          "description": "待转换的文件后缀"
        },
        "inputDir": {
          "type": "string",
          "default": "src",
          "description": "待转换的文件存放的目录"
        },
        "outputDir": {
          "type": "string",
          "default": "dist",
          "description": "转换后的文件存放的目录"
        }
      }
    },
    "commands": [
      {
        "command": "transform.addExport",
        "title": "addExport"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.44.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.1.1",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0",
    "gogoast": "0.0.5"
  },
  "dependencies": {
    "gogoast": "0.0.5"
  }
}
